{
  "name": "reflection-js",
  "version": "2.0.1",
  "description": "Module reflection API for nodejs",
  "main": "start.js",
  "scripts": {
    "eslint": "eslint .",
    "test": "npm run eslint && npm run cover",
    "mocha": "_mocha -b",
    "cover": "nyc --temp-directory coverage/.nyc_output --reporter text-summary --reporter html --reporter lcov  mocha",
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/131/reflection.git"
  },
  "keywords": [
    "reflection"
  ],
  "author": "Francois Leurent <131.code@leurent.email>",
  "license": "ISC",
  "dependencies": {
    "acorn": "^8.0.5",
    "mout": "^1.0.0",
    "nyks": "^5.11.6"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^2.2.5",
    "eslint-plugin-ivs": "^1.3.0",
    "nyc": "^13.0.1"
  }
}
